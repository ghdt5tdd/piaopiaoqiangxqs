        <view class='my-bar-name'>订单查询</view>
        <image class='my-bar-next' src='../../images/w_next.png'></image>
      </view>
    </navigator> -->
    <navigator url="../order/order" hover-class='none'>
      <view class='my-bar'>
        <image class='my-bar-icon' src='../../images/my9.png'></image>
        <view class='my-bar-name'>预约订单</view>
        <image class='my-bar-next' src='../../images/w_next.png'></image>
      </view>
    </navigator>
    <navigator url="../signorder/signorder" hover-class='none'>
      <view class='my-bar'>
        <image class='my-bar-icon' src='../../images/my8.png'></image>
        <view class='my-bar-name'>货运单签收</view>
        <image class='my-bar-next' src='../../images/w_next.png'></image>
      </view>
    </navigator>
    <view class='my-bar' bindtap='toShopOrder' data-type = '0'>
      <image class='my-bar-icon' src='../../images/my10.png'></image>
      <view class='my-bar-name'>查收货</view>
      <image class='my-bar-next' src='../../images/w_next.png'></image>
    </view>
    <view class='my-bar' bindtap='toShopOrder' data-type = '1'>
      <image class='my-bar-icon' src='../../images/my11.png'></image>
      <view class='my-bar-name'>查发货</view>
      <image class='my-bar-next' src='../../images/w_next.png'></image>
    </view>
  </view>

  <view class='card-title'>工具中心</view>
  <view class='card-order'>
    <navigator url="../truck/truck" hover-class='none'>
      <view class='my-bar'>
        <image class='my-bar-icon' src='../../images/my6.png'></image>
        <view class='my-bar-name'>货车定位</view>
        <image class='my-bar-next' src='../../images/w_next.png'></image>
      </view>
    </navigator>
